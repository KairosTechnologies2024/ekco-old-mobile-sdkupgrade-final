{
  "expo": {
    "name": "Ekco Vehicle Tracker",
    "slug": "ekco-mobile-old-sdk-upgrade",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ekcomobileoldsdkupgrade",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios":
     {
      "supportsTablet": true,
      "bundleIdentifier": "com.ekcovercel.com",
      "googleServicesFile": "./GoogleService-Info.plist",
       "jsEngine": "jsc",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
      
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ekcovercel.com",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "VIBRATE",
        "com.google.android.c2dm.permission.RECEIVE",
        "POST_NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD8uGqzqokt3i354ZgBZoZb5TywYwhGG_E"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ekco logo white.png",
          "resizeMode": "contain",
          "backgroundColor": "#182f51"
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "android_google_services_file": "./google-services.json",
          "ios_google_services_file": "./GoogleService-Info.plist"
        }
      ],
      [
        "@react-native-firebase/messaging",
        {
          "ios_auto_register_for_remote_messages": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {
 "useFrameworks": "static",
      "useModularHeaders": true,
        "useReactNativePods": true
      
          }
        }
      ]

    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0117a74a-71cc-488c-94d0-99892f07b0f1"
      }
    }
  }
}
